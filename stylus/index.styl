@css {/*  CSS3 Clases - Luis Gago */}
@import 'nib/vendor'
@import 'nib/gradients'
@import 'nib/overflow'
@import 'nib/normalize'
@import 'nib/flex'
vendor-prefixes = o ms moz webkit
border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  ms-border-radius arguments
  o-border-radius arguments
  border-radius arguments
appearance()
  -webkit-appearance arguments
  -moz-appearance arguments
  ms-appearance arguments
  o-appearance arguments
  appearance arguments
box-shadow()
  -webkit-box-shadow arguments
  -moz-box-shadow arguments
  ms-box-shadow arguments
  o-box-shadow arguments
  box-shadow arguments
transform()
  -webkit-transform arguments
  -moz-transform arguments
  ms-transform arguments
  o-transform arguments
  transform arguments
transition()
  -webkit-transition arguments
  -moz-transition arguments
  ms-transition arguments
  o-transition arguments
  transition arguments
flex()
  -webkit-box-flex arguments
  -moz-box-flex arguments
  -webkit-flex arguments
  -ms-flex arguments
  o-flex arguments
  flex arguments
flexbox()
  display: -webkit-box
  display: -moz-box
  display: -ms-flexbox
  display: -webkit-flex
  display: flex
box-shadow()
  -webkit-box-shadow arguments
  -moz-box-shadow arguments
  ms-box-shadow arguments
  o-box-shadow arguments
  box-shadow arguments
zoom()
  -ms-zoom arguments
  -webkit-zoom arguments
  zoom arguments
background-size()
  -webkit-background-size arguments
  -moz-background-size arguments
  ms-background-size arguments
  o-background-size arguments
  background-size arguments
user-select()
  -webkit-user-select arguments
  -moz-user-select arguments
  -ms-user-select arguments
  user-select arguments
box-sizing()
  -webkit-box-sizing arguments
  -moz-box-sizing arguments
  -ms-box-sizing arguments
  box-sizing arguments
opacity(n)
  opacity n
  if support-for-ie
    filter unquote('progid:DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')
*, *:after, *:before
  box-sizing border-box
  margin 0
//@import 'fonts'
body
  font-size: 16px
//Estructura
.page
    width: 1000px
    margin: auto
    background: #333
    color #FFF
h1
  color #333
